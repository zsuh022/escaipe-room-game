      </Pane>
      <ImageView fx:id="smallKeyInserter" fitHeight="42.0" fitWidth="53.0" layoutX="367.0" layoutY="240.0" onMouseClicked="#openKeyInserter" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/keyinserter.png" />
         </image>
      </ImageView>
      <Label fx:id="timeLabel" layoutX="410.0" layoutY="71.0" prefHeight="42.0" prefWidth="137.0">
         <font>
            <Font size="31.0" />
         </font>
      </Label>
      <Button layoutX="278.0" layoutY="45.0" mnemonicParsing="false" onAction="#room1ButtonClicked" text="room1" />
